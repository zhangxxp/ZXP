package com.sc.pojo;

import java.io.Serializable;

public class SPower implements Serializable {
    private Integer id;

    private Short systemstate;

    private Short semstate;

    private Short classstate;

    private Short studentsstate;

    private Short jobstate;

    private static final long serialVersionUID = 1L;

    public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public Short getSystemstate() {
        return systemstate;
    }

    public void setSystemstate(Short systemstate) {
        this.systemstate = systemstate;
    }

    public Short getSemstate() {
        return semstate;
    }

    public void setSemstate(Short semstate) {
        this.semstate = semstate;
    }

    public Short getClassstate() {
        return classstate;
    }

    public void setClassstate(Short classstate) {
        this.classstate = classstate;
    }

    public Short getStudentsstate() {
        return studentsstate;
    }

    public void setStudentsstate(Short studentsstate) {
        this.studentsstate = studentsstate;
    }

    public Short getJobstate() {
        return jobstate;
    }

    public void setJobstate(Short jobstate) {
        this.jobstate = jobstate;
    }

    @Override
    public String toString() {
        StringBuilder sb = new StringBuilder();
        sb.append(getClass().getSimpleName());
        sb.append(" [");
        sb.append("Hash = ").append(hashCode());
        sb.append(", id=").append(id);
        sb.append(", systemstate=").append(systemstate);
        sb.append(", semstate=").append(semstate);
        sb.append(", classstate=").append(classstate);
        sb.append(", studentsstate=").append(studentsstate);
        sb.append(", jobstate=").append(jobstate);
        sb.append("]");
        return sb.toString();
    }
}